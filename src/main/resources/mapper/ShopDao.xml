<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
    
<mapper namespace="com.imooc.o2o.dao.ShopDao" >
	<insert id="insertShop">
		insert into tb_shop(shop_id,owner_id,area_id,shop_category_id,shop_name,shop_desc,shop_addr,phone,
							shop_img,priority,create_time,enable_status,advice)
							value(#{shopId}#{owner.userId}#{area.areaId},#{shopCategory.shopCategoryId},#{shop_name},#{shop_desc}
							,#{shop_addr},#{PersonInfo.phone},#{shop_img},#{priority},#{create_time},#{enable_status},#{advice})
	</insert>	
</mapper>      	

 	